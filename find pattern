import re
def count_substring(string, sub_string):
    sub_string= f'(?={sub_string})'
    a= re.findall(sub_string, string)
    return len(a)

if __name__ == '__main__':
    string = input().strip()
    sub_string = input().strip()
    
    count = count_substring(string, sub_string)
    print(count)
