def minion_game(string):
    stuart = 0
    kevin = 0
    l = len(string)
    for i in range(l):
        if string[i].lower() in ['a', 'e', 'o', 'u', 'i']:
            kevin += (l - i)
        else:
            stuart += (l - i)

    if stuart > kevin:
        print(f"Stuart {stuart}")
    elif kevin > stuart:
        print(f"Kevin {kevin}")
    else:
        print("Draw")


if __name__ == '__main__':
    s = input()
    minion_game(s)
